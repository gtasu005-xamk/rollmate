{
  "routes": [
    {
      "route": "/health",
      "rewrite": "https://rollmate-swe-dev-api.azurewebsites.net/health"
    },
    {
      "route": "/auth/*",
      "rewrite": "https://rollmate-swe-dev-api.azurewebsites.net/auth/*"
    },
    {
      "route": "/sessions/*",
      "rewrite": "https://rollmate-swe-dev-api.azurewebsites.net/sessions/*"
    },
    {
      "route": "/themes/*",
      "rewrite": "https://rollmate-swe-dev-api.azurewebsites.net/themes/*"
    },
    {
      "route": "/tasks/*",
      "rewrite": "https://rollmate-swe-dev-api.azurewebsites.net/tasks/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/health", "/auth/*", "/sessions/*", "/themes/*", "/summary/*", "/tasks/*"]
  }
}